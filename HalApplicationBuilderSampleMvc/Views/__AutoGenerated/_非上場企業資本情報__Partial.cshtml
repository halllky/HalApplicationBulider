
@model HalApplicationBuilderSampleMvc.Models.非上場企業資本情報;

<div class="halapp-aggregate-container">
    @if (Model.halapp_fields.IsRoot == false) {
        <input type="button" value="削除" class="halapp-remove-button halapp-btn-secondary" />
    }

    
<div class="flex flex-col">

    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            主要株主
        </label>
        <div class="flex-1">
        
            <input asp-for="主要株主" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            安定性
        </label>
        <div class="flex-1">
        
            <input asp-for="安定性" class="border" />


        </div>
    </div>

</div>


    <input type="hidden" asp-for="halapp_fields.IsRoot" class="" />
    <input type="hidden" asp-for="halapp_fields.Removed" class="halapp-removed" />
</div>
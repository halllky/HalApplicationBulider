
@model HalApplicationBuilderSampleMvc.Models.非上場企業資本情報;

<div class="halapp-aggregate-container">
    @if (Model.__halapp__.IsRoot == false) {
        <input type="button" value="削除" class="halapp-remove-button" />
    }

    
<div class="card container">
    <div class="card-body form-horizontal">
        <div class="row mb-3">
            <label class="col-sm-2 col-form-label">
                主要株主
            </label>
            <div class="col-sm-10">
                <input asp-for="主要株主"/>
            </div>
        </div>
        <div class="row mb-3">
            <label class="col-sm-2 col-form-label">
                安定性
            </label>
            <div class="col-sm-10">
                <input asp-for="安定性"/>
            </div>
        </div>
    </div>
</div>


    <input type="hidden" asp-for="__halapp__.Removed" class="halapp-removed" />
</div>

@model HalApplicationBuilderSampleMvc.Models.会社;

<div class="halapp-aggregate-container">
    @if (Model.halapp_fields.IsRoot == false) {
        <input type="button" value="削除" class="halapp-remove-button halapp-btn-secondary" />
    }

    
<div class="flex flex-col">

    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            会社ID
        </label>
        <div class="flex-1">
        
            <input asp-for="会社ID" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            会社名
        </label>
        <div class="flex-1">
        
            <input asp-for="会社名" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            連絡先
        </label>
        <div class="flex-1">
        
            
<div class="flex flex-col">

    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            電話番号
        </label>
        <div class="flex-1">
        
            <input asp-for="連絡先.電話番号" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            郵便番号
        </label>
        <div class="flex-1">
        
            <input asp-for="連絡先.郵便番号" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            住所1
        </label>
        <div class="flex-1">
        
            <input asp-for="連絡先.住所1" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            住所2
        </label>
        <div class="flex-1">
        
            <input asp-for="連絡先.住所2" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            住所3
        </label>
        <div class="flex-1">
        
            <input asp-for="連絡先.住所3" class="border" />


        </div>
    </div>

</div>

            

        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            資本情報
        </label>
        <div class="flex-1">
        
            <div>
                <label>
                    <input type="radio" asp-for="資本情報" value="0" />
                    上場企業資本情報
                </label>
                
<div class="flex flex-col">

    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            自己資本比率
        </label>
        <div class="flex-1">
        
            <input asp-for="資本情報_上場企業資本情報.自己資本比率" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            利益率
        </label>
        <div class="flex-1">
        
            <input asp-for="資本情報_上場企業資本情報.利益率" class="border" />


        </div>
    </div>

</div>

            </div>
            <div>
                <label>
                    <input type="radio" asp-for="資本情報" value="1" />
                    非上場企業資本情報
                </label>
                
<div class="flex flex-col">

    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            主要株主
        </label>
        <div class="flex-1">
        
            <input asp-for="資本情報_非上場企業資本情報.主要株主" class="border" />


        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            安定性
        </label>
        <div class="flex-1">
        
            <input asp-for="資本情報_非上場企業資本情報.安定性" class="border" />


        </div>
    </div>

</div>

            </div>
            

        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
    
        <label class="w-32 select-none">
            営業所
        </label>
        <div class="flex-1">
        
            @for (var i = 0; i < Model.営業所.Count; i++) {
                <partial name="_営業所__Partial.cshtml" for="営業所[i]" />
            }
            <input
                type="button"
                value="追加"
                class="halapp-add-child halapp-btn-secondary"
                halapp-aggregate-tree-path="会社.営業所"
                halapp-aggregate-model-path="営業所" />
                

        </div>
    </div>

</div>


    <input type="hidden" asp-for="halapp_fields.IsRoot" class="" />
    <input type="hidden" asp-for="halapp_fields.Removed" class="halapp-removed" />
</div>

@model HalApplicationBuilderSampleMvc.Models.上場企業資本情報;

<div class="halapp-aggregate-container">
    @if (Model.__halapp__.IsRoot == false) {
        <input type="button" value="削除" class="halapp-remove-button halapp-btn-secondary" />
    }

    
<div class="border border-gray-300 rounded-lg flex flex-col p-2">
    <div class="flex flex-col md:flex-row mb-1">
        <label class="w-32 select-none">
            自己資本比率
        </label>
        <div class="flex-1">
            <input asp-for="自己資本比率"/>
        </div>
    </div>
    <div class="flex flex-col md:flex-row mb-1">
        <label class="w-32 select-none">
            利益率
        </label>
        <div class="flex-1">
            <input asp-for="利益率"/>
        </div>
    </div>
</div>


    <input type="hidden" asp-for="__halapp__.Removed" class="halapp-removed" />
</div>
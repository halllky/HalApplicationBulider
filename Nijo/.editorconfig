# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.cs]
end_of_line = crlf
charset = utf-8
indent_size = 4
brace_style = end_of_line
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,jsx,ts,tsx,cjs,css,html,xml,json,gitignore,md}]
end_of_line = lf
charset = utf-8
indent_size = 2
brace_style = end_of_line
trim_trailing_whitespace = true
insert_final_newline = true

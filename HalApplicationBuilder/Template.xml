<?xml version="1.0" encoding="UTF-8" ?>
<!--アプリケーション名を変更してください。-->
<YourApplicationName>

  <!--アプリケーションのデータ構造を定義してください。-->

  <!--列挙体を定義してください。-->
  <_Enum>

  </_Enum>

  <!--ソースコードの自動生成に関する設定-->
  <_CodeGen>
    <!--出力先ディレクトリ-->
    <OutDirRoot>./YourApplicationDirectory</OutDirRoot>
    <OutDirRelativePath>
      <EFCore>EntityFramework/__AutoGenerated</EFCore>
      <MvcModel>Models/__AutoGenerated</MvcModel>
      <MvcView>Views/_AutoGenerated</MvcView>
      <MvcController>Controllers/__AutoGenerated</MvcController>
    </OutDirRelativePath>

    <!--出力時名前空間-->
    <Namespace>
      <DbContextNamespace>NamespaceOfYourApplication.EntityFramework</DbContextNamespace>
      <EntityNamespace>NamespaceOfYourApplication.EntityFramework.Entities</EntityNamespace>
      <MvcModelNamespace>NamespaceOfYourApplication.Models</MvcModelNamespace>
      <MvcControllerNamespace>NamespaceOfYourApplication.Controllers</MvcControllerNamespace>
    </Namespace>

    <!--DbContextクラス名-->
    <DbContextName>YourDbContextClassName</DbContextName>
  </_CodeGen>
</YourApplicationName>
